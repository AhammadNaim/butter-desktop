{
  "name": "butter-component-list",
  "version": "0.6.0",
  "description": "A base React Component for Butter Project",
  "main": "dist/",
  "build": {
    "command": "webpack --config webpack.config.js --progress --profile --colors",
    "env": {
      "NODE_ENV": "production"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buttercomponents/butter-component-list.git"
  },
  "keywords": [
    "react",
    "components",
    "butterproject",
    "butter"
  ],
  "author": "Niv Sardi <x@btn.sh>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/buttercomponents/butter-component-list/issues"
  },
  "homepage": "https://github.com/buttercomponents/butter-component-list#readme",
  "devDependencies": {
    "butter-component-builder": "buttercomponents/butter-component-builder"
  },
  "dependencies": {
    "butter-base-components": "buttercomponents/butter-base-components",
    "butter-component-list": "buttercomponents/butter-component-list",
    "butter-component-menu": "buttercomponents/butter-component-menu",
    "butter-component-movie-details": "buttercomponents/butter-component-movie-details",
    "butter-component-settings": "buttercomponents/butter-component-settings",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1"
  },
  "scripts": {
    "prepublish": "npm run build",
    "install": "PATH=$PATH:../.bin bcb-run install",
    "build": "bcb-run build",
    "start": "bcb-run start",
    "lint": "bcb-run lint"
  }
}
